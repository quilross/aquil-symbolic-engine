{
  "name": "aquil-prod",
  "version": "2.0.0",
  "description": "Personal AI Wisdom Builder & Trust Reinforcement System",
  "main": "backend/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "setup": "node setup.js",
    "tail": "wrangler tail",
    "db:init": "wrangler d1 execute AQUIL_DB --file=schema.sql --env production",
    "db:migrate": "wrangler d1 migrations apply AQUIL_DB --env production",
    "db:migrate-fix": "node scripts/active/migrate-d1.mjs",
    "types": "wrangler types",
    "test": "node --test test/*.test.js",
    "guard": "npm run guard:schema && npm run guard:patterns && npm run guard:aliases",
    "guard:schema": "node scripts/active/check-schema-consistency.mjs",
    "guard:patterns": "node scripts/active/check-banned-patterns.mjs",
    "guard:aliases": "node scripts/active/check-op-aliases.mjs",
    "reconcile": "node scripts/active/reconcile-logs.mjs",
    "accept": "node scripts/active/acceptance.mjs",
    "smoke-logs": "node scripts/active/smoke-logs.mjs",
    "spec:diff": "node scripts/active/spec-diff.mjs",
    "spec:bump": "node scripts/active/bump-openapi-version.mjs",
    "spec:commit": "node -e \"process.exit(0)\"",
    "ci:spec-bump-check": "node scripts/active/check-spec-bump.mjs",
    "audit:comprehensive": "node scripts/active/comprehensive-audit.mjs",
    "audit:fix-suggestions": "node scripts/active/fix-suggestions.mjs"
  },
  "keywords": [
    "personal-ai",
    "trust-building",
    "wisdom-extraction",
    "somatic-healing",
    "standing-tall",
    "symbolic-engine",
    "aquil",
    "human-design",
    "gene-keys",
    "astrology"
  ],
  "author": "Aquil Signal-Q",
  "license": "Personal",
  "dependencies": {
    "itty-router": "^5.0.22"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250831.0",
    "axios": "^1.11.0",
    "knip": "^5.63.1",
    "node-fetch": "^3.3.2",
    "wrangler": "^4.33.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quilross/aquil-symbolic-engine.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
