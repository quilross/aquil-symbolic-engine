{
  "name": "aquil-prod",
  "version": "2.0.0",
  "description": "Personal AI Wisdom Builder & Trust Reinforcement System",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "setup": "node setup.js",
    "tail": "wrangler tail",
    "db:init": "wrangler d1 execute AQUIL_DB --file=schema.sql --env production",
    "db:migrate": "wrangler d1 migrations apply AQUIL_DB --env production",
    "db:migrate-fix": "node scripts/migrate-d1.mjs",
    "types": "wrangler types",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:legacy": "node --test test/*.test.js",
    "test:integration": "vitest run test/integration.test.js",
    "test:miniflare": "node scripts/test-miniflare.mjs",
    "guard": "npm run guard:schema && npm run guard:patterns && npm run guard:aliases",
    "guard:schema": "node scripts/check-schema-consistency.mjs",
    "guard:patterns": "node scripts/check-banned-patterns.mjs",
    "guard:aliases": "node scripts/check-op-aliases.mjs",
    "reconcile": "node scripts/reconcile-logs.mjs",
    "accept": "node scripts/acceptance.mjs",
    "smoke-logs": "node scripts/smoke-logs.mjs",
    "spec:diff": "node scripts/spec-diff.mjs",
    "spec:bump": "node scripts/bump-openapi-version.mjs",
    "spec:commit": "node -e \"process.exit(0)\"",
    "ci:spec-bump-check": "node scripts/check-spec-bump.mjs",
    "audit:comprehensive": "node scripts/comprehensive-audit.mjs",
    "audit:fix-suggestions": "node scripts/fix-suggestions.mjs"
  },
  "keywords": [
    "personal-ai",
    "trust-building",
    "wisdom-extraction",
    "somatic-healing",
    "standing-tall",
    "symbolic-engine",
    "aquil",
    "human-design",
    "gene-keys",
    "astrology"
  ],
  "author": "Aquil Signal-Q",
  "license": "Personal",
  "dependencies": {
    "itty-router": "^5.0.22"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.70",
    "@cloudflare/workers-types": "^4.20250831.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.2",
    "@opentelemetry/sdk-node": "^0.204.0",
    "@vitest/coverage-v8": "^3.2.4",
    "ajv": "^8.17.1",
    "axios": "^1.11.0",
    "miniflare": "^4.20250902.0",
    "node-fetch": "^3.3.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.33.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quilross/aquil-symbolic-engine.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
