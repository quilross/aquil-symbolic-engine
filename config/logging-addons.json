{
  "logging-schema-addons": {
    "/api/analytics/insights": {
      "get": {
        "summary": "Get conversation analytics and insights",
        "description": "Automatically called to analyze conversation patterns and provide usage insights.",
        "operationId": "getConversationAnalytics",
        "x-openai-autonomous": true,
        "parameters": [
          {
            "name": "timeframe",
            "in": "query",
            "description": "Analysis timeframe (day, week, month)",
            "schema": { "type": "string", "enum": ["day", "week", "month"], "default": "week" }
          }
        ],
        "responses": {
          "200": {
            "description": "Conversation analytics and patterns",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "conversation_patterns": { "type": "array", "items": { "type": "string" } },
                    "growth_indicators": { "type": "array", "items": { "type": "string" } },
                    "engagement_metrics": { "type": "object" },
                    "insights_summary": { "type": "string" },
                    "recommendations": { "type": "array", "items": { "type": "string" } }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/export/conversation": {
      "post": {
        "summary": "Export conversation data",
        "description": "Export conversation history in various formats for backup or analysis.",
        "operationId": "exportConversationData",
        "x-openai-isConsequential": true,
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "format": { "type": "string", "enum": ["json", "csv", "pdf"], "default": "json" },
                  "date_range": { "type": "object" },
                  "include_analytics": { "type": "boolean", "default": true }
                },
                "required": ["format"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Exported conversation data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "export_url": { "type": "string" },
                    "file_size": { "type": "string" },
                    "record_count": { "type": "integer" },
                    "export_id": { "type": "string" }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/webhooks/register": {
      "post": {
        "summary": "Register webhook for real-time notifications",
        "description": "Set up webhook endpoints for real-time event notifications.",
        "operationId": "registerWebhook",
        "x-openai-isConsequential": true,
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "webhook_url": { "type": "string", "format": "uri" },
                  "events": { "type": "array", "items": { "type": "string" } },
                  "secret": { "type": "string" }
                },
                "required": ["webhook_url", "events"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Webhook registration confirmation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "webhook_id": { "type": "string" },
                    "status": { "type": "string" },
                    "events_registered": { "type": "array", "items": { "type": "string" } }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
