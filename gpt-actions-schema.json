          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "type": { "type": "string", "default": "chat_message" },
                  "who": { "type": "string", "default": "user" },
                  "level": { "type": "string", "default": "info" },
                  "payload": { "type": "object" }
                },
                "required": ["payload"]
              }
            }
          }
        },
        "responses": {
          "200": { "description": "Log stored successfully" }
        }
      }
    },
