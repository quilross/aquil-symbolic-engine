{
  "openapi": "3.1.0",
  "info": {
    "title": "Aquil Production API (Complete Infrastructure)",
    "description": "Personal AI Wisdom Builder with full Cloudflare Worker bindings support. Includes D1, KV, R2, Vectorize, and AI infrastructure plus personal growth tools.",
    "version": "2.3.0"
  },
  "servers": [
    {
      "url": "https://signal-q.me",
      "description": "Aquil's personal AI domain (Production)"
    }
  ],
  "paths": {
    "/api/commitments/create": {
      "post": {
        "summary": "Create and manage personal commitments",
        "description": "Called to create or update personal commitments and micro-practices.",
        "operationId": "manageCommitment",
        "x-openai-isConsequential": true,
        "x-openai-autonomous": true,
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "action": { "type": "string", "enum": ["create", "update"] },
                  "commitment": { "type": "string" },
                  "timeframe": { "type": "string" },
                  "micro_practice": { "type": "boolean" }
                },
                "required": ["action", "commitment"]
              }
            }
          }
        },
        "responses": {
          "200": { "description": "Commitment management results" }
        }
      }
    },
    "/api/commitments/active": {
      "get": {
        "summary": "List active commitments",
        "description": "Called to retrieve current active commitments and their progress.",
        "operationId": "listActiveCommitments",
        "x-openai-autonomous": true,
        "responses": {
          "200": { "description": "Active commitments list" }
        }
      }
    },
    "/api/commitments/progress": {
      "post": {
        "summary": "Update commitment progress",
        "description": "Called to update progress on existing commitments.",
        "operationId": "updateCommitmentProgress",
        "x-openai-isConsequential": true,
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "commitment_id": { "type": "string" },
                  "progress": { "type": "string" },
                  "completion_status": { "type": "string", "enum": ["in_progress", "completed", "paused"] }
                },
                "required": ["commitment_id", "progress"]
              }
            }
          }
        },
        "responses": {
          "200": { "description": "Progress update results" }
        }
      }
    },
    "/api/d1/query": {
      "post": {
        "summary": "Direct D1 database queries",
        "description": "Execute SQL queries against the D1 database. Use for complex data operations.",
        "operationId": "queryD1Database",
        "x-openai-isConsequential": true,
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "sql": { "type": "string" },
                  "params": { "type": "array", "items": { "type": "string" } }
                },
                "required": ["sql"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Query results",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "results": { "type": "array", "items": { "type": "object" } },
                    "success": { "type": "boolean" },
                    "meta": { "type": "object" }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/kv/log": {
      "post": {
        "summary": "Store data in KV storage",
        "description": "Direct KV storage for fast key-value data access.",
        "operationId": "storeInKV",
        "x-openai-isConsequential": true,
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "key": { "type": "string" },
                  "value": { "type": "object" },
                  "expirationTtl": { "type": "integer" }
                },
                "required": ["key", "value"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "KV storage result",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": { "type": "boolean" },
                    "key": { "type": "string" }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/vectorize/upsert": {
      "post": {
        "summary": "Store vectors in the index",
        "description": "Add or update vectors in the vectorize index.",
        "operationId": "upsertVectors",
        "x-openai-isConsequential": true,
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "vectors": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "id": { "type": "string" },
                        "values": { "type": "array", "items": { "type": "number" } },
                        "metadata": { "type": "object" }
                      }
                    }
                  },
                  "namespace": { "type": "string" }
                },
                "required": ["vectors"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Upsert results",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "upsertedCount": { "type": "integer" },
                    "success": { "type": "boolean" }
                  }
                }
              }
            }
          }
        }
      }
    },
