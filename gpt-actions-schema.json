{
  "openapi": "3.1.0",
  "info": {
    "title": "Aquil Symbolic Engine API",
    "description": "Personal AI Wisdom Builder & Trust Reinforcement System - Standing tall in the world, rooted in internal trust",
    "version": "2.0.0"
  },
  "servers": [
    {
      "url": "https://signal-q.me",
      "description": "Aquil's personal AI domain"
    }
  ],
  "paths": {
      "/api/log": {
        "post": {
            "summary": "Automatic Log Data or Event (GPT can use anytime)",
            "description": "Automatically log any data, message, or event for context continuity, metrics, or error tracking. This action is available for GPT to use at any time, without restriction, for flexible and autonomous logging.",
          "operationId": "logDataOrEvent",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "type": { "type": "string", "description": "Type of log (e.g. chat_message, metric, error, context, etc.)" },
                    "who": { "type": "string", "description": "Who or what generated the log (user, assistant, system, etc.)" },
                    "level": { "type": "string", "description": "Severity or importance (info, warning, error, etc.)" },
                    "session_id": { "type": "string", "description": "Session or conversation identifier" },
                    "tags": { "type": "array", "items": { "type": "string" }, "description": "Tags for filtering and context (optional)" },
                    "idx1": { "type": "string", "description": "Custom index field 1 (optional)" },
                    "idx2": { "type": "string", "description": "Custom index field 2 (optional)" },
                    "payload": { "type": "object", "description": "Main log data, can be any JSON object or string" }
                  },
                  "required": ["type", "payload"]
                }
              }
            }
          },
          "responses": {
            "200": { "description": "Log entry created with unique id" }
          }
        }
      },
      "/api/logs": {
        "get": {
            "summary": "Automatic Retrieve Logs or Data Entries (GPT can use anytime)",
            "description": "Automatically retrieve logs for context, continuity, or analysis. This action is available for GPT to use at any time, including at the start of every conversation for frame of reference, and for ongoing autonomous retrieval.",
          "operationId": "retrieveLogsOrDataEntries",
          "parameters": [
            { "name": "limit", "in": "query", "schema": { "type": "integer", "default": 20, "minimum": 1, "maximum": 200 }, "description": "Max number of logs to retrieve" },
            { "name": "type", "in": "query", "schema": { "type": "string" }, "description": "Type of log to filter (optional)" },
            { "name": "who", "in": "query", "schema": { "type": "string" }, "description": "Who generated the log (optional)" },
            { "name": "level", "in": "query", "schema": { "type": "string" }, "description": "Log level to filter (optional)" },
            { "name": "session_id", "in": "query", "schema": { "type": "string" }, "description": "Session or conversation id (optional)" },
            { "name": "tag", "in": "query", "schema": { "type": "string" }, "description": "Tag to filter (optional)" },
            { "name": "since", "in": "query", "schema": { "type": "string", "format": "date-time" }, "description": "Start time (optional)" },
            { "name": "until", "in": "query", "schema": { "type": "string", "format": "date-time" }, "description": "End time (optional)" },
            { "name": "idx1", "in": "query", "schema": { "type": "string" }, "description": "Custom index field 1 (optional)" },
            { "name": "idx2", "in": "query", "schema": { "type": "string" }, "description": "Custom index field 2 (optional)" }
          ],
          "responses": {
            "200": { "description": "Array of log entries matching filters" }
          }
        }
      },
      "/api/session-init": {
        "get": {
            "summary": "Automatic Retrieve Recent Logs for Session Initialization (GPT can use anytime)",
            "description": "Automatically retrieve the most recent logs at the start of a new conversation to provide continuity and frame of reference. This action is available for GPT to use at any time, for autonomous session bootstrapping and context gathering.",
          "operationId": "retrieveRecentSessionLogs",
          "parameters": [
            { "name": "limit", "in": "query", "schema": { "type": "integer", "default": 7, "minimum": 1, "maximum": 200 }, "description": "Number of recent logs to retrieve (default 7)" },
            { "name": "type", "in": "query", "schema": { "type": "string" }, "description": "Type of log to filter (optional)" },
            { "name": "session_id", "in": "query", "schema": { "type": "string" }, "description": "Session or conversation id (optional)" }
          ],
          "responses": {
            "200": { "description": "Array of recent log entries for session initialization" }
          }
        }
      },
    "/api/trust/check-in": {
      "post": {
        "summary": "Trust Building Check-In Session",
        "description": "Personal trust-building companion for standing tall in the world",
        "operationId": "trustCheckIn",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "current_state": {
                    "type": "string",
                    "description": "How are you feeling about trusting yourself right now?"
                  },
                  "trust_level": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 10,
                    "description": "Trust level 1-10"
                  },
                  "specific_situation": {
                    "type": "string",
                    "description": "Specific situation where you're questioning self-trust"
                  },
                  "body_sensations": {
                    "type": "string", 
                    "description": "What is your body telling you?"
                  }
                },
                "required": ["current_state"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Personalized trust guidance with exercises"
          }
        }
      }
    },
    "/api/media/extract-wisdom": {
      "post": {
        "summary": "Transform Media into Personal Wisdom",
        "description": "Extract wisdom from TV, movies, books, podcasts, music",
        "operationId": "extractMediaWisdom",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "media_type": {
                    "type": "string",
                    "enum": ["tv_show", "movie", "book", "podcast", "music", "article"],
                    "description": "Type of content consumed"
                  },
                  "title": {
                    "type": "string",
                    "description": "Title of the content"
                  },
                  "your_reaction": {
                    "type": "string",
                    "description": "Your personal reaction, thoughts, or feelings"
                  },
                  "content_summary": {
                    "type": "string",
                    "description": "Brief summary (optional)"
                  }
                },
                "required": ["media_type", "title", "your_reaction"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Wisdom extraction with growth insights"
          }
        }
      }
    },
    "/api/somatic/session": {
      "post": {
        "summary": "Body Wisdom & Somatic Healing Session",
        "description": "Connect with your body's intelligence for trust building",
        "operationId": "somaticHealingSession",
        "x-openai": {
          "description": "Use this when the user wants to work with body sensations, emotions, or needs somatic healing practices. Perfect for connecting with body wisdom and releasing tension."
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SomaticSessionRequest"
              },
              "examples": {
                "tension_release": {
                  "summary": "Working with body tension",
                  "value": {
                    "body_state": "Shoulders feel tight and raised, jaw is clenched",
                    "emotions": "Stress and overwhelm",
                    "intention": "Release tension and feel more grounded"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Personalized somatic healing session"
          }
        }
      }
    },
    "/api/wisdom/synthesize": {
      "post": {
        "summary": "Multi-Framework Wisdom Synthesis",
        "description": "Guidance from Human Design, Gene Keys, astrology, somatic wisdom",
        "operationId": "synthesizeWisdom",
        "x-openai": {
          "description": "Use this when the user has a specific life question or decision and wants integrated guidance from multiple wisdom frameworks like Human Design, Gene Keys, astrology, and somatic wisdom."
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "life_situation": {
                    "type": "string",
                    "description": "Your current life situation"
                  },
                  "specific_question": {
                    "type": "string",
                    "description": "Specific question or decision you're facing"
                  },
                  "frameworks_requested": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "enum": ["human_design", "gene_keys", "astrology", "somatic_wisdom", "all"]
                    }
                  }
                },
                "required": ["life_situation", "specific_question"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Synthesized wisdom from multiple frameworks"
          }
        }
      }
    },
    "/api/patterns/recognize": {
      "post": {
        "summary": "Pattern Recognition & Growth Insights",
        "description": "Identify patterns in your trust journey and growth",
        "operationId": "recognizePatterns",
        "x-openai": {
          "description": "Use this to analyze patterns in the user's growth journey, recurring themes, or to celebrate progress over time. Great for reflection and recognizing evolution."
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "area_of_focus": {
                    "type": "string",
                    "enum": ["trust_building", "media_consumption", "body_awareness", "overall_growth"]
                  },
                  "recent_experiences": {
                    "type": "string",
                    "description": "Recent experiences you've been navigating"
                  },
                  "recurring_themes": {
                    "type": "string",
                    "description": "Any recurring themes you've noticed"
                  }
                },
                "required": ["area_of_focus", "recent_experiences"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Pattern analysis with growth recommendations"
          }
        }
      }
    },
    "/api/standing-tall/practice": {
      "post": {
        "summary": "Standing Tall Practice Session",
        "description": "Stand tall in the world instead of shrinking",
        "operationId": "standingTallPractice",
        "x-openai": {
          "description": "Use this when the user mentions feeling small, shrinking, lacking confidence, or wanting to practice standing in their power. Perfect for building authentic empowerment."
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "situation": {
                    "type": "string",
                    "description": "Situation where you tend to shrink instead of standing tall"
                  },
                  "fears_concerns": {
                    "type": "string",
                    "description": "Fears or concerns around standing in your power"
                  },
                  "desired_outcome": {
                    "type": "string",
                    "description": "How would you like to show up?"
                  },
                  "past_successes": {
                    "type": "string",
                    "description": "Times when you did stand tall successfully"
                  }
                },
                "required": ["situation", "desired_outcome"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Standing tall practices with confidence building"
          }
        }
      }
    },
    "/api/wisdom/daily-synthesis": {
      "get": {
        "summary": "Daily Wisdom Compilation",
        "description": "Your personalized daily wisdom and growth insights",
        "operationId": "getDailySynthesis",
        "x-openai": {
          "description": "Use this to provide daily wisdom compilation, morning insights, or when the user wants their personalized wisdom summary."
        },
        "parameters": [
          {
            "name": "period",
            "in": "query",
            "schema": {
              "type": "string",
              "enum": ["today", "week", "month"],
              "default": "today"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Personalized wisdom compilation"
          }
        }
      }
    }
  }
}
