# Aquil Symbolic Engine Environment Variables
# Copy this file to .env and update with your actual values

# Cloudflare API Token (required for deployment and vector operations)
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token-here

# Development Server Configuration
# Used by test scripts and local development
DEV_SERVER_URL=http://localhost:8787
ACCEPTANCE_URL=http://localhost:8787

# Feature Flags
ENABLE_CANARY=false
CANARY_PERCENT=5
DISABLE_NEW_MW=false
ENABLE_CONVERSATIONAL_ENGINE=1
GPT_COMPAT_MODE=0

# Rate Limiting Configuration
ENABLE_RATE_LIMIT=true
RATE_LIMIT_RPS=10
RATE_LIMIT_BURST=20

# Request Size Limits
ENABLE_REQ_SIZE_CAP=true
REQ_SIZE_BYTES=2000000
MAX_PAYLOAD_BYTES=16384

# Environment Settings
ENVIRONMENT=development
TIMEZONE=America/New_York
AUTO_SESSION_INIT=true
VOICE_ADAPTATION=true

# Vector API Key (uses Cloudflare API token by default)
VECTOR_API_KEY=${CLOUDFLARE_API_TOKEN}